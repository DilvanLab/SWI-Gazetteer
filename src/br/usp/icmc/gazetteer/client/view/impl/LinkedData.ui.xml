<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>

	</ui:style>
	<g:HorizontalPanel width="100%" height="100%">
		<g:VerticalPanel width="100%" height="100%" >
			<g:Label text="Place:"/>
			<g:Label text="Place: Fazenda santa maria." ui:field="place_text"/>
			<g:Label text="County: Itacoatiara" ui:field="county_place"/>
			<g:Label text="Species found: 458" ui:field="species_text"/>
			<g:HorizontalPanel>
				<g:Hyperlink text="RDF" targetHistoryToken="newHistoryToken" ui:field="rdf_link"/>
				<g:Hyperlink text="GML" targetHistoryToken="newHistoryToken" ui:field="gm_link"/>
				<g:Hyperlink text="JSON" targetHistoryToken="newHistoryToken" ui:field="json_link"/>
			</g:HorizontalPanel>
			<g:Label text="Contributors number:"/>
			<g:Label text="12 peoples" ui:field="contributors_number"/>
			<g:Label text="agreement about Linked Data: 0%" ui:field="agreement"/>
			<g:VerticalPanel height="30%" width="100%" ui:field="graph_agr"/>
		</g:VerticalPanel>
		<g:VerticalPanel width="100%" height="100%" >
			<g:Label text="External Link:"/>
			<g:Hyperlink text="FarmDBpedia" targetHistoryToken="newHistoryToken" ui:field="external_link"/>
			<g:Image ui:field="image"/>
			<g:TextArea text="Description" width="100%" height="100%" ui:field="description_text"/>
			<g:VerticalPanel>
				<g:HorizontalPanel height="" width="">
				<g:Label text="Point:"/>
				<g:SimpleCheckBox checked="true" ui:field="yes_point"/>
				<g:Label text="Yes"/>
			</g:HorizontalPanel>
				<g:Label text="Point( -3.071 -59.356)" ui:field="point_text"/>
				<g:HorizontalPanel>
				<g:Label text="Polygon:"/>
				<g:SimpleCheckBox ui:field="yes_polygon"/>
				<g:Label text="Yes"/>
			</g:HorizontalPanel>
				<g:Label text="Polygon not found" ui:field="polygon_text"/>
			</g:VerticalPanel>
		</g:VerticalPanel>
	</g:HorizontalPanel>
</ui:UiBinder> 